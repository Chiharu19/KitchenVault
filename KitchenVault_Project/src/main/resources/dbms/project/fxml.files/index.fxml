<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dbms.project.kitchenvault_project.controllers.introController">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Kitchen Vault">
         <font>
            <Font name="SansSerif Regular" size="40.0" />
         </font>
      </Text>
      <AnchorPane maxWidth="-Infinity" minWidth="-Infinity" prefHeight="310.0" prefWidth="312.0">
         <children>
            <Button fx:id="adminBtn" layoutX="14.0" layoutY="133.0" mnemonicParsing="false" prefHeight="44.0" prefWidth="90.0" text="Admin">
               <font>
                  <Font name="SansSerif Regular" size="18.0" />
               </font>
            </Button>
            <Button fx:id="deliveryBtn" layoutX="207.0" layoutY="133.0" mnemonicParsing="false" onAction="#showDeliveryInterface" prefHeight="44.0" prefWidth="90.0" text="Delivery">
               <font>
                  <Font name="SansSerif Regular" size="18.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</VBox>
